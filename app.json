{
  "expo": {
    "name": "pr3",
    "slug": "pr3",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.todoapp.midterm",
      "buildNumber": "1.0.0",
      "icon": "./assets/ios-icon.png"
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "dev.todoapp.midterm",
      "versionCode": 1,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ],
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ]
  }
}